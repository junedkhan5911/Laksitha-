#include <stdio.h>

// Function prototypes
int check_rows(int grid[9][9]);
int check_columns(int grid[9][9]);
int check_subgrids(int grid[9][9]);

int main() {
    // Hard-coded valid Sudoku solution
    int grid[9][9] = {
        {5,3,4,6,7,8,9,1,2},
        {6,7,2,1,9,5,3,4,8},
        {1,9,8,3,4,5,6,7,9}, // <- This row has a mistake (duplicate 9)
        {8,5,9,7,6,1,4,2,3},
        {4,2,6,8,5,3,7,9,1},
        {7,1,3,9,2,4,8,5,6},
        {9,6,1,5,3,7,2,8,4},
        {2,8,7,4,1,9,5,3,6},
        {3,4,5,2,8,6,1,7,9}
    };

    // Call checking functions
    int r = check_rows(grid);
    int c = check_columns(grid);
    int s = check_subgrids(grid);

    if (r && c && s) {
        printf("VALID SOLUTION\n");
    } else {
        printf("INVALID SOLUTION\n");
    }

    return 0;
}

// Check rows
int check_rows(int grid[9][9]) {
    for (int i = 0; i < 9; i++) {
        int seen[10] = {0};
        for (int j = 0; j < 9; j++) {
            int num = grid[i][j];
            if (num < 1 || num > 9) return 0;
            if (seen[num]) return 0;
            seen[num] = 1;
        }
    }
    return 1;
}

// Check columns
int check_columns(int grid[9][9]) {
    for (int j = 0; j < 9; j++) {
        int seen[10] = {0};
        for (int i = 0; i < 9; i++) {
            int num = grid[i][j];
            if (num < 1 || num > 9) return 0;
            if (seen[num]) return 0;
            seen[num] = 1;
        }
    }
    return 1;
}

// Check 3x3 subgrids
int check_subgrids(int grid[9][9]) {
    for (int row = 0; row < 9; row += 3) {
        for (int col = 0; col < 9; col += 3) {
            int seen[10] = {0};
            for (int i = 0; i < 3; i++) {
                for (int j = 0; j < 3; j++) {
                    int num = grid[row + i][col + j];
                    if (num < 1 || num > 9) return 0;
                    if (seen[num]) return 0;
                    seen[num] = 1;
                }
            }
        }
    }
    return 1;
}
